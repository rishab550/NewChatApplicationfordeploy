{
  "version": 2,
  "builds": [
    {
      "src": "frontend/vite-project/vite.config.js",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "frontend/vite-project/dist"  
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "frontend/vite-project/dist/$1"
    }
  ]
}
